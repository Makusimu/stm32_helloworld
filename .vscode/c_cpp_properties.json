{
    "env": {
        "project-includes": [
            "${workspaceFolder}",
            "${workspaceFolder}/system/inc",
            "x:/#Embedded/programs/arm-gcc/arm-none-eabi/include"
        ],
        "arm-gcc": "x:/#Embedded/programs/arm-gcc"
    },
    "configurations": [
        {
            "name": "Embedded",
            "intelliSenseMode": "gcc-x86",
            "includePath": [
                "${project-includes}"
            ],
            "defines": [
                "STM32F103xB"
            ],
            "compilerPath": "${arm-gcc}",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}